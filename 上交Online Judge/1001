#include <bits/stdc++.h>
using namespace std;

int main()
{
    int personHeight,deskHeight,n;
    cin>>personHeight>>deskHeight>>n;
    int apple[n];
    for(int i=0;i<n;++i)
    {
        cin>>apple[i];
    }

    int count=0;
    for(int i=0;i<n;++i)
    {
        if(personHeight+deskHeight>=apple[i])
        {
            ++count;
        }
    }
    cout<<count<<endl;

    return 0;
}

